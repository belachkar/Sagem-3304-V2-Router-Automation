{
	"manifest_version": 2,
	"name": "Sagem 3304 V2 Router Automation",
	"short_name": "Sagem3304V2",
	"description": "Automate Sagem 3304 V2 Router operations",
	"version": "0.0.1",
	"icons": {
		"128": "img/icons/img_128.png",
		"64": "img/icons/img_64.png",
		"48": "img/icons/img_48.png",
		"32": "img/icons/img_32.png",
		"24": "img/icons/img_24.png",
		"16": "img/icons/img_16.png"
	},
	"browser_action": {
		"default_icon": {
			"128": "img/icons/img_128.png",
			"64": "img/icons/img_64.png",
			"48": "img/icons/img_48.png",
			"32": "img/icons/img_32.png",
			"24": "img/icons/img_24.png",
			"16": "img/icons/img_16.png"
		},
		"default_title": "Sagem 3304 V2 Router Automation",
		"default_popup": "popup.html"
	},
	"permissions": [ "<all_urls>" ],
	"background": {
		"scripts": [
			"js/lib/variables.js",
			"js/lib/config.js",
			"js/shared/messages.js",
			"js/shared/handlers.js",
			"js/lib/utils.js",
			"js/lib/hashPass.js",
			"js/lib/actions.js",
			"js/lib/app.js",
			"js/bg.js"
		]
	},
	"content_scripts": [
		{
			"matches": [ "<all_urls>" ],
			"js": [
				"js/ext/jquery.min.js",
				"js/ext/popper.min.js",
				"js/ext/bootstrap.min.js",
				"js/shared/messages.js",
				"js/shared/handlers.js",
				"js/ext/toastMessage.js",
				"js/contentScript.js"
			]
		}
	]
}
